{
  "compilerOptions": {
    "lib": ["deno.window"],
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "types": []
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}

